{
  "name": "notes",
  "productName": "Notes",
  "private": true,
  "version": "0.6.7",
  "description": "A minimalist notes app for capturing quick thoughts and ideas",
  "author": "cletus",
  "license": "MIT",
  "main": "dist/main/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/calledforth/note-app.git"
  },
  "scripts": {
    "dev": "concurrently \"pnpm run dev:vite\" \"pnpm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "tsc && vite build && electron-builder --config electron-builder.json",
    "build:win": "tsc && vite build && electron-builder --win --config electron-builder.json",
    "build:dir": "tsc && vite build && electron-builder --dir --config electron-builder.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "publish": "tsc && vite build && electron-builder --win --publish always --config electron-builder.json"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "better-sqlite3": "^12.5.0",
    "clsx": "^2.1.1",
    "electron-updater": "^6.6.2",
    "eventemitter3": "^5.0.1",
    "fast-equals": "^5.3.2",
    "framer-motion": "^12.23.26",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.561.0",
    "parchment": "^3.0.0",
    "prop-types": "^15.8.1",
    "quill": "^2.0.3",
    "quill-delta": "^5.1.0",
    "re-resizable": "^6.11.2",
    "react": "18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "18.2.0",
    "react-draggable": "^4.5.0",
    "react-grid-layout": "^1.5.2",
    "react-quill": "^2.0.0",
    "react-resizable": "^3.0.5",
    "react-resizable-panels": "^3.0.6",
    "react-rnd": "^10.5.2",
    "react-router-dom": "^7.9.5",
    "resize-observer-polyfill": "^1.5.1",
    "scheduler": "^0.27.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.6.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-grid-layout": "^1.3.5",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.1.2",
    "electron": "^39.1.1",
    "electron-builder": "^25.1.8",
    "electron-updater": "^6.6.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "react-scan": "^0.4.3",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6",
    "wait-on": "^8.0.3"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48",
  "pnpm": {
    "overrides": {
      "@npmcli/move-file": "^3.0.0",
      "glob": "^10.0.0",
      "rimraf": "^5.0.0"
    },
    "onlyBuiltDependencies": [
      "electron",
      "better-sqlite3"
    ]
  }
}
